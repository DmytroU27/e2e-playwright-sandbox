{
  "compileOnSave": false,
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": false,
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "outDir": "../out-tsc/e2e",
    "module": "commonjs",
    "target": "es2018",
    "types": ["node"],
    "typeRoots": ["./node_modules/@types", "./typings"],
    "baseUrl": ".",
    "paths": {
      "string-format": ["typings/string-format"],
      "pkijs": ["typings/pkijs"]
    }
  }
}
